    "grunt-cli": "^1.4.3",
    "grunt-contrib-qunit": "^4.0.0",
    "grunt-contrib-uglify": "^5.0.1",
    "grunt-run": "^0.8.1",
    "@nevware21/grunt-ts-plugin": "^0.3.0",
    "typedoc": "^0.23.25",
